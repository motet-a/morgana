{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-console": [0],
    "no-param-reassign": [0],
    "consistent-return": [0],
    "global-require": [0],
    "max-len": [0]
  },
  "env": {
    "browser": false,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
